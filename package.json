{
  "name": "Hissues",
  "version": "1.0.0",
  "description": "The ultimate solution to your housing issues",
  "main": "index.js",
  "dependencies": {
    "chalk": "^2.3.0",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "es6-promisify": "^5.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "http": "0.0.0",
    "mongoose": "^5.0.3",
    "mongoose-mongodb-errors": "0.0.2",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.4.0",
    "pug": "^2.0.0-rc.4",
    "slug": "^0.9.1",
    "supervisor": "^0.12.0",
    "validator": "^9.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.1",
    "dotenv": "^4.0.0",
    "eslint": "^5.7.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.3.0",
    "mocha": "^4.0.1",
    "nock": "^9.1.5",
    "prettier": "^1.14.3",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "super-request": "^1.2.0"
  },
  "scripts": {
    "start": "node index.js",
    "develop": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cross-env NODE_ENV=development supervisor -w web,models index.js",
    "server:prod": "cross-env NODE_ENV=production node index.js",
    "client": "node start-client.js",
    "clientv2": "cd frontend && npm start",
    "lint": "esw web frontend",
    "lint:watch": "npm run lint -- --watch",
    "test:unit:fe": "cd frontend && npm run test:batch",
    "test:unit:models": "mocha --reporter progress ./test/setup.js ./models/**/*.unit.spec.js",
    "test:unit": "mocha --reporter progress ./test/setup.js ./**/*.unit.spec.js",
    "test:unit:dev": "mocha --watch --reporter progress ./test/setup.js ./**/*.unit.spec.js",
    "test:e2e": "mocha --reporter progress ./test/setup.js ./**/*.e2e.spec.js",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test": "npm run test:unit:models && cd frontend && npm run test:batch",
    "heroku-prebuild": "cd frontend && npm install && npm run build",
    "prepushnojsx": "npm run lint && npm run test",
    "postmergee": "./scripts/githooks/post-merge.sh",
    "prepushe": "npm run lint && npm run test",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chingu-voyage3/bears-21.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chingu-voyage3/bears-21/issues"
  },
  "homepage": "https://github.com/chingu-voyage3/bears-21#readme"
}
